{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "windows"],
		"quotes": ["error", "single"],
		"jsx-quotes": ["error", "prefer-double"],
		"semi": ["warn", "always"],
		"no-console": "warn",
		"prefer-const": "error",
		"max-len": [
			"error",
			{
				"code": 120
			}
		],
		"no-unused-vars": "warn"
	}
}
